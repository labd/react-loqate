    "cssnano": "^5.1.14",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "msw": "^0.49.3",
    "postcss": "^8.4.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "size-limit": "^8.1.2",
